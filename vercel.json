{
  "version": 2,
  "buildCommand": "next build",
  "outputDirectory": ".next",
  "github": {
    "silent": true
  },
  "env": {
    "VERCEL_IGNORE_BUILD_ERRORS": "true"
  },
  "functions": {
    "app/api/**/*.js": {
      "runtime": "nodejs20.x"
    },
    "app/api/**/*.ts": {
      "runtime": "nodejs20.x"
    }
  }
}